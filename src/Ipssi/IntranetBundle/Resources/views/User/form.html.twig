{{ form_start(form) }}
<div class="form-group">
    {{ form_row(form.profile.firstname) }}
</div>
<div class="form-group">
    {{ form_row(form.profile.lastname) }}
</div>
<div class="form-group">
    {{ form_row(form.profile.pseudo, {'attr': {'class': 'form-control'}}) }}
</div>
<div class="form-group">
    {{ form_row(form.username) }}
</div>
<div class="form-group">
    {{ form_row(form.password) }}
</div>
<div class="form-group">
    {{ form_row(form.email) }}
</div>
<div class="form-group">
    {{ form_row(form.profile.phone) }}
</div>
<div class="form-group">
    {{ form_row(form.profile.birthDate) }}
</div>
<div class="form-group">
    {{ form_row(form.profile.address) }}
</div>
<div class="form-group">
    {{ form_row(form.enabled) }}
</div>
{{ form_end(form) }}

